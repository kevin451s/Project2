<div class="form">
  <div class="imgcontainer">
    <h1>Log In</h1>
  </div>

<%= form_for :login do |f| %>
  <div class="container">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="container">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <button>
      <%= f.submit 'Log In' %>
    </button>
  </div>
<% end %>

</div>
